<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>Userlogin</servlet-name>
        <servlet-class>Login.Userlogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>Login.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>Login.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddCustomerServlet</servlet-name>
        <servlet-class>Customer_details.AddCustomerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FlightBookingServlet</servlet-name>
        <servlet-class>Booking.FlightBookingServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PaymentServlet</servlet-name>
        <servlet-class>Booking.PaymentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PassengerDetailsServlet</servlet-name>
        <servlet-class>Booking.PassengerDetailsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PNRDetailsServlet</servlet-name>
        <servlet-class>Booking.PNRDetailsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StorePassengerDetailsServlet</servlet-name>
        <servlet-class>Booking.StorePassengerDetailsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PNRCheckServlet</servlet-name>
        <servlet-class>Booking.PNRCheckServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CancelTicketServlet</servlet-name>
        <servlet-class>Booking.CancelTicketServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PaymentStatusServlet</servlet-name>
        <servlet-class>Booking.PaymentStatusServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FlightStatusServlet</servlet-name>
        <servlet-class>Home.FlightStatusServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Userlogin</servlet-name>
        <url-pattern>/Userlogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/RegisterServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddCustomerServlet</servlet-name>
        <url-pattern>/AddCustomerServlet</url-pattern>
    </servlet-mapping>
</web-app>
